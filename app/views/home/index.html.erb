<% if !user_signed_in? %>
            <%= link_to 'sign up', new_user_registration_path, class: "rounded-lg  bg-blue-700 text-white px-5 py-2.5 font-semibold text-sm hover:bg-blue-800 flex items-center" %>
            <%= link_to 'log in', new_user_session_path, class: "rounded-lg  bg-blue-700 text-white px-5 py-2.5 font-semibold text-sm hover:bg-blue-800 flex items-center"%>
          <% else %>
            <%= link_to current_user.email, edit_user_registration_path, data: { "turbo-method": :patch },class: "" %>
            <%= link_to "log out", destroy_user_session_path, data: { "turbo-method": :delete }, class: "rounded-lg  bg-blue-700 text-white px-5 py-2.5 font-semibold text-sm hover:bg-blue-800 flex items-center" %>
          <% end %>

<div>
  <h1 class="font-bold text-4xl">Home#index</h1>
  <p>Find me in app/views/home/index.html.erb</p>
</div>
